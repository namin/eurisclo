  [LAMBDA (X)                                               (* edited: " 4-MAR-81 16:08")
    X])

(GeneralizeDataType
  [LAMBDA (x tmp)                                           (* edited: "25-MAR-81 12:39")
    (COND
      [(LISTP x)
	(MAPCAR x (FUNCTION (LAMBDA (Z)
		    (COND
		      ((RandomP)
			(GeneralizeDataType Z))
		      (T Z]
      ((SETQ tmp (RandomChoose (Generalizations x)))
	(SETQ UDiff (LIST x RArrow tmp))
	tmp)
      (T x])

