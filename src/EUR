				   (MergeTasks
				     [MAPCAR NeedSpec
					     (FUNCTION
					       (LAMBDA
						 (ns)
						 (LIST (Half CurPri)
						       ns
						       (QUOTE Specializations)
						       [LIST (CONS CurUnit
								   (APPEND (QUOTE (might have been 
										      specialized 
											 better, 
											 earlier, if 
											 some 
										  specializations had 
											 existed for))
									   (LIST ns]
						       (LIST (LIST (QUOTE CreditTo)
								   (QUOTE H6]
				     Agenda))
			     [AND NeedSpec
				  (SETQ TaskResults
					(AddPropL TaskResults (QUOTE NewTasks)
						  (CONS NeedSpec
							(APPEND (QUOTE (will be specialized, because 
