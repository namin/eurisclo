(RPAQQ MinPri 150)

(RPAQQ MoveDefns ((MOVD (QUOTE AND)
			(QUOTE AND-2)
			T)
		  (MOVD (QUOTE AND)
			(QUOTE AND-1)
			T)
		  (MOVD (QUOTE AND)
			(QUOTE AND-1)
			T)
		  (MOVD (QUOTE BestSubset)
			(QUOTE BestSubset-3)
			T)
		  (MOVD (QUOTE BestSubset)
			(QUOTE BestSubset-2)
			T)
		  (MOVD (QUOTE BestSubset)
			(QUOTE BestSubset-1)
			T)
		  (MOVD (QUOTE AND)
			(QUOTE AND-2)
			T)
		  (MOVD (QUOTE AND)
			(QUOTE AND-1)
			T)))

